{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "lib": ["es7"],
        "allowJs": true,
        "outDir": "dist",
        "rootDir": "src",
        "strict": true,
        "noImplicitAny": false,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "skipLibCheck": true
    }
}
